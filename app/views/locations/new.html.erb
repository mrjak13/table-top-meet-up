<h1>New Location here!</h1>

<%= form_for Location.new do |f| %>
	Name: <%= f.text_field :name %><br>
	Address 1: <%= f.text_field :address_1 %><br>
	Address 2: <%= f.text_field :address_2 %><br>
	City: <%= f.text_field :city %><br>
	State: <%= f.text_field :state %><br>
	Zip: <%= f.text_field :zip %><br><br>
	<%= f.submit "Create Location" %>
<% end %>